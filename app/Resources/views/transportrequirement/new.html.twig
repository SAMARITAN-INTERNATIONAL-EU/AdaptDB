{% extends 'base.html.twig' %}

{% block title %}New Transport Requirement{% endblock %}


{% block body %}
    <div class="row">
        <div class="col-lg-6">
            <h1>New Transport Requirement</h1>

            {{ form_start(form, {'attr': {'class': 'form'}}) }}
            {{ form_label(form.name) }}
            {{ form_widget(form.name, {'attr': {'class': 'form-control textBox_withMarginBottom'}}) }}

            <input class="btn btn-primary" type="submit" value="Create" />
            <a class="btn btn-default" href="{{ path('transportrequirement_index') }}">Cancel</a>

            {{ form_end(form) }}

        </div>
    </div>
{% endblock %}
